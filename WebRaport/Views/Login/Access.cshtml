@{
    ViewData["Title"] = "Access denied!";
}
<div class="content-wrapper" style="min-height: 1192.4px">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>@ViewData["Title"]</h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a asp-controller="Home" asp-action="Index">Главная</a></li>
                        <li class="breadcrumb-item active">@ViewData["Title"]</li>
                    </ol>
                </div>
            </div>
        </div><!-- /.container-fluid -->
    </section>

    <section class="content">
        <div class="error-page">
            <h2 class="headline text-warning"> Access denied!</h2>

            <div class="error-content">
                <h3><i class="fas fa-exclamation-triangle text-warning"></i> Уппс! У вас нет доступа!.</h3>

                <p>
                    К сожалению у вас нет разрешений для просмотра данного контента!
                </p>

            </div>
            <!-- /.error-content -->
        </div>
    </section>
</div>


